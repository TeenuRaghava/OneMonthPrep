def timeConversion(s):
    # Write your code here
    if s[8]=="A":
        if(s[:2]=="12"):
            return("00"+s[2:8])
        else:
            return(s[:8])
    else:
        if(s[:2]=="12"):
            return(s[:8])
        else:
            n=int(s[:2])+12
            return(str(n)+s[2:8])
